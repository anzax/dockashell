{
  "name": "dockashell",
  "version": "0.1.1-beta.0",
  "description": "MCP server that gives every AI Agent its own isolated Docker container",
  "type": "module",
  "bin": {
    "dockashell": "./src/cli/cli.js"
  },
  "scripts": {
    "dev:tui": "node src/tui/tui-launcher.js",
    "start": "node src/mcp/mcp-server.js",
    "test": "node --test test/**/*.test.js",
    "test:integration": "node test/integration/test-*.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:md": "prettier --write '**/*.md'",
    "format:check": "prettier --check .",
    "build-image": "node src/cli/cli.js build",
    "rebuild-image": "node src/cli/cli.js build --force",
    "release:beta": "release-it --preRelease=beta",
    "release:patch": "npm run release:beta",
    "release:minor": "release-it minor --npm.tag=latest",
    "release:major": "release-it major --npm.tag=latest",
    "release:stable": "release-it --npm.tag=latest"
  },
  "dependencies": {
    "@inkjs/ui": "^2.0.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@nanostores/react": "^1.0.0",
    "chokidar": "^4.0.3",
    "commander": "^14.0.0",
    "dockerode": "^4.0.0",
    "fs-extra": "^11.0.0",
    "ink": "^5.2.1",
    "lodash.throttle": "^4.1.1",
    "nanostores": "^1.0.1",
    "picocolors": "^1.1.1",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "mcp",
    "docker",
    "ai",
    "agent",
    "container"
  ],
  "author": "https://github.com/anzax",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/anzax/dockashell.git"
  },
  "homepage": "https://github.com/anzax/dockashell#readme",
  "bugs": {
    "url": "https://github.com/anzax/dockashell/issues"
  },
  "files": [
    "src/",
    "docker/",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.2.0",
    "ink-testing-library": "^4.0.0",
    "prettier": "^3.5.3",
    "release-it": "^19.0.3"
  }
}
